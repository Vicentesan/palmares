{
  "name": "${appName}",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "require": "./dist/index.cjs",
  "scripts": {
    "lint": "eslint .",
    "makemigrations": "drizzle-kit generate",
    "migrate": "tsx migrate.ts",
    "dev": "tsx ./src/index.ts",
    "build": "tsup index.ts"
  },
  "dependencies": {
    "@palmares/databases": "${@palmares/core/version}",
    "@palmares/drizzle-engine": "${@palmares/drizzle-engine/version}",
    "@palmares/node-std": "${@palmares/node-std/version}"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@eslint/js": "latest",
    "globals": "latest",
    "eslint": "latest",
    "tsup": "latest",
    "typescript": "latest",
    "typescript-eslint": "latest",
    "tsx": "latest"
  }
}
